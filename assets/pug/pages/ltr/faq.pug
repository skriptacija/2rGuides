- var src = "color-1.css"
- var inner = "index.html"
doctype html
html(lang='en')
    head
      include ../../_assets/ltr/includes/header
    body.light-footer
      include ../../_assets/ltr/components/loader
      include ../../_assets/ltr/includes/nav
      .page-margin
      // breadcrumb start
      .breadcrumb-bg
        .container
          .row
            .col-md-6.col-sm-6.col-text-center.d-align-center
              h2.title
                span FAQ
            .col-md-6.col-sm-6.col-text-center
              nav.blog-bradcrumb(aria-label='breadcrumb')
                ol.breadcrumb.bg-transparent.mb-0
                  li.breadcrumb-item
                    a(href='index.html') Home
                  li.breadcrumb-item.active
                    a(href='#') FAQ
      // breadcrumb end
      section
        .innerpage-decor
          .innerpage-circle1
            img(src='../assets/images/Testimonial2.png', alt='')
          .innerpage-circle2
            img(src='../assets/images/Testimonial1.png', alt='')
